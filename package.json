{
  "name": "headway-shipwright-token-style",
  "version": "1.0.0",
  "description": "to be determined",
  "main": "main.ts",
  "type": "module",
  "scripts": {
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "prepare": "husky install",
    "build-tailwind": "node --experimental-specifier-resolution=node --loader ts-node/esm main.ts tokens/raw/tokens.json build tailwind",
    "build-mui": "node --experimental-specifier-resolution=node --loader ts-node/esm main.ts tokens/raw/tokens.json build mui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomxkay/style-dictionary.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tomxkay/style-dictionary/issues"
  },
  "homepage": "https://github.com/tomxkay/style-dictionary#readme",
  "dependencies": {
    "husky": "^8.0.2",
    "style-dictionary": "^3.7.1",
    "token-transformer": "^0.0.27",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.3",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "autoprefixer": "^10.4.12",
    "babel-jest": "^29.3.1",
    "eslint": "^8.25.0",
    "jest": "^29.3.1",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^15.0.0",
    "prepush-if-changed": "^1.0.8",
    "tailwindcss": "^3.1.8",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ]
  },
  "husky": {
    "hooks": {
        "pre-push": "npm test && prepush-if-changed"
    }
  },
  "prepush-if-changed": {
    "example/project-nextjs-tailwind/*": "cd example/project-nextjs-tailwind && npm test",
    "example/project-nextjs-mui/*": "example/project-nextjs-mui && npm test",
    "example/project-elixir": "example/project-elixir-tailwind && mix test"
  }
}
