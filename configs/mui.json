{
  "source": ["tokens/transformed/**/*.json"],
  "platforms": {
    "mui": {
      "transformGroup": "js/mui",
      "buildPath": "./build/mui/",
      "files": [
        {
          "destination": "typography.json",
          "format": "muiTypography"
        },
        {
          "destination": "misc.json",
          "format": "muiMisc"
        },
        {
          "destination": "colors.json",
          "format": "muiColors"
        },
        {
          "destination": "shadows.json",
          "format": "muiShadows"
        }
      ]
    }
  }
}
